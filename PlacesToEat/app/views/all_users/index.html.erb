<h1>AllUsers#index</h1>
<p>Find me in app/views/all_users/index.html.erb</p>


<ul class="users">
  <% @users.each do |user| %>
      <li>
        <%=link_to user.name, controller: "api/users", action: "show", id: user.id %>
        <%= user.lastname %>
        - <%= user.email %>
      </li>
  <% end %>
</ul>